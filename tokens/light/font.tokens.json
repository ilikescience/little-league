{
  "$extensions": { "mode": "light" },
  "color": {
    "$type": "color",
    "font": {
      "primary": {
        "30": { "$value": "{color.core.gray.90}" },
        "40": { "$value": "{color.core.gray.80}" },
        "50": { "$value": "{color.core.gray.70}" },
        "60": { "$value": "{color.core.gray.60}" },
        "70": { "$value": "{color.core.gray.70}" }
      },
      "ghost": {
        "$value": {
          "colorSpace": "srgb",
          "components": [0.796078, 0.796078, 0.8],
          "alpha": 1,
          "hex": "#CBCBCC"
        }
      },
      "inverse": { "$value": "{color.background.surface.20}" },
      "active": { "$value": "{color.core.blue.80}" },
      "success": { "$value": "{color.core.green.80}" },
      "danger": { "$value": "{color.core.red.80}" },
      "warning": { "$value": "{color.core.coral.80}" },
      "info": { "$value": "{color.core.aqua.80}" },
      "update": { "$value": "{color.core.mint.80}" },
      "link": {
        "primary": {
          "active": { "$value": "{color.core.blue.60}" },
          "inactive": {
            "$value": {
              "colorSpace": "srgb",
              "components": [0.415686, 0.545098, 0.901961],
              "alpha": 1,
              "hex": "#6A8BE6"
            }
          }
        }
      },
      "code": {
        "primary": {
          "foreground": { "$value": "{color.core.gray.80}" },
          "background": { "$value": "{color.core.gray.05}" },
          "red": { "$value": "{color.core.red.60}" },
          "purple": { "$value": "{color.core.purple.60}" },
          "blue": { "$value": "{color.core.blue.60}" },
          "aqua": { "$value": "{color.core.aqua.70}" },
          "teal": { "$value": "{color.core.teal.60}" },
          "mint": { "$value": "{color.core.mint.60}" },
          "green": { "$value": "{color.core.green.80}" },
          "olive": { "$value": "{color.core.olive.80}" },
          "yellow": { "$value": "{color.core.yellow.70}" },
          "coral": { "$value": "{color.core.coral.60}" }
        },
        "secondary": {
          "foreground": { "$value": "{color.core.gray.70}" },
          "background": { "$value": "{color.core.gray.10}" },
          "red": { "$value": "{color.core.red.50}" },
          "purple": { "$value": "{color.core.purple.50}" },
          "blue": { "$value": "{color.core.blue.50}" },
          "aqua": { "$value": "{color.core.aqua.60}" },
          "teal": { "$value": "{color.core.teal.50}" },
          "mint": { "$value": "{color.core.mint.50}" },
          "green": { "$value": "{color.core.green.40}" },
          "olive": { "$value": "{color.core.olive.70}" },
          "yellow": { "$value": "{color.core.yellow.60}" },
          "coral": { "$value": "{color.core.coral.50}" }
        }
      }
    }
  }
}
