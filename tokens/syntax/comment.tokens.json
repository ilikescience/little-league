{
  "syntax": {
    "$type": "color",
    "comment": {
      "*": { "$value": "{color.font.primary.70}" },
      "line": {
        "$value": "{color.font.primary.70}",
        "$extensions": { "fontStyle": "italic" }
      },
      "block": {
        "documentation": { "$value": "{color.font.primary.60}" }
      },
      "single": { "$value": "{color.font.primary.70}" }
    }
  }
}
