{
  "$extensions": { "mode": "dark" },
  "color": {
    "$type": "color",
    "font": {
      "primary": {
        "30": { "$value": "{color.core.gray.00}" },
        "40": { "$value": "{color.core.gray.05}" },
        "50": { "$value": "{color.core.gray.10}" },
        "60": { "$value": "{color.core.gray.20}" },
        "70": { "$value": "{color.core.gray.50}" }
      },
      "ghost": {
        "$value": {
          "colorSpace": "srgb",
          "components": [0.360784, 0.360784, 0.384314],
          "alpha": 1,
          "hex": "#5C5C62"
        }
      },
      "inverse": { "$value": "{color.background.surface.20}" },
      "active": { "$value": "{color.core.blue.40}" },
      "success": { "$value": "{color.core.green.40}" },
      "danger": { "$value": "{color.core.red.40}" },
      "warning": { "$value": "{color.core.coral.40}" },
      "info": { "$value": "{color.core.aqua.40}" },
      "update": { "$value": "{color.core.mint.40}" },
      "link": {
        "primary": {
          "active": { "$value": "{color.core.blue.50}" },
          "inactive": {
            "$value": {
              "colorSpace": "srgb",
              "components": [0.345098, 0.494118, 0.807843],
              "alpha": 1,
              "hex": "#587ECE"
            }
          }
        }
      },
      "code": {
        "primary": {
          "foreground": { "$value": "{color.core.gray.10}" },
          "background": { "$value": "{color.core.gray.90}" },
          "red": { "$value": "{color.core.red.60}" },
          "purple": { "$value": "{color.core.purple.40}" },
          "blue": { "$value": "{color.core.blue.40}" },
          "aqua": { "$value": "{color.core.aqua.50}" },
          "teal": { "$value": "{color.core.teal.60}" },
          "mint": { "$value": "{color.core.mint.40}" },
          "green": { "$value": "{color.core.green.30}" },
          "olive": { "$value": "{color.core.olive.50}" },
          "yellow": { "$value": "{color.core.yellow.40}" },
          "coral": { "$value": "{color.core.coral.50}" }
        },
        "secondary": {
          "foreground": { "$value": "{color.core.gray.00}" },
          "background": { "$value": "{color.core.gray.95}" },
          "red": { "$value": "{color.core.red.50}" },
          "purple": { "$value": "{color.core.purple.50}" },
          "blue": { "$value": "{color.core.blue.50}" },
          "aqua": { "$value": "{color.core.aqua.40}" },
          "teal": { "$value": "{color.core.teal.50}" },
          "mint": { "$value": "{color.core.mint.50}" },
          "green": { "$value": "{color.core.green.50}" },
          "olive": { "$value": "{color.core.olive.40}" },
          "yellow": { "$value": "{color.core.yellow.30}" },
          "coral": { "$value": "{color.core.coral.40}" }
        }
      }
    }
  }
}
