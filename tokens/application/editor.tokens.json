{
  "$type": "color",
  "editor": {
    "background": { "$value": "{color.background.surface.30}" },
    "foreground": { "$value": "{color.font.primary.50}" },
    "selectionBackground": { "$value": "{color.background.selection.active.20}" },
    "selectionHighlightBackground": { "$value": "{color.background.selection.inactive.30}" },
    "inactiveSelectionBackground": { "$value": "{color.background.selection.inactive.30}" },
    "wordHighlightBackground": { "$value": "{color.background.selection.inactive.10}" },
    "wordHighlightStrongBackground": { "$value": "{color.background.selection.inactive.0}" },
    "findMatchBackground": { "$value": "{color.background.highlight.primary.active}" },
    "findMatchHighlightBackground": { "$value": "{color.background.selection.inactive.20}" },
    "findRangeHighlightBackground": {},
    "hoverHighlightBackground": { "$value": "{color.background.selection.inactive.20}" },
    "lineHighlightBackground": { "$value": "{color.background.surface.20-alpha-50}" },
    "lineHighlightBorder": {},
    "rangeHighlightBackground": { "$value": "{color.background.surface.10-alpha-50}" },
    "focusedStackFrameHighlightBackground": {},
    "stackFrameHighlightBackground": {},
    "snippetTabstopHighlightBackground": {},
    "snippetTabstopHighlightBorder": {},
    "snippetFinalTabstopHighlightBackground": {},
    "snippetFinalTabstopHighlightBorder": {}
  },
  "editorLineNumber": {
    "foreground": { "$value": "{color.font.primary.70}" },
    "activeForeground": { "$value": "{color.font.primary.60}" }
  },
  "editorCursor": {
    "foreground": { "$value": "{color.font.primary.60}" }
  },
  "editorLink": {
    "activeForeground": { "$value": "{color.font.link.primary.inactive}" }
  },
  "editorGutter": {
    "background": { "$value": "{color.background.surface.30}" },
    "modifiedBackground": { "$value": "{color.border.info}" },
    "addedBackground": { "$value": "{color.border.success}" },
    "deletedBackground": { "$value": "{color.border.danger}" }
  },
  "editorOverviewRuler": {
    "border": { "$value": "{color.border.primary.60}" },
    "findMatchForeground": { "$value": { "colorSpace": "srgb", "components": [0, 1, 0], "alpha": 0.8, "hex": "#00ff00" } },
    "rangeHighlightForeground": { "$value": { "colorSpace": "srgb", "components": [0, 0, 1], "alpha": 0.8, "hex": "#0000ff" } },
    "selectionHighlightForeground": { "$value": "{color.background.selection.inactive.20}" },
    "currentContentForeground": { "$value": { "colorSpace": "srgb", "components": [1, 1, 1], "alpha": 0.8, "hex": "#ffffff" } },
    "incomingContentForeground": { "$value": { "colorSpace": "srgb", "components": [0, 1, 0], "alpha": 0.8, "hex": "#00ff00" } },
    "commonContentForeground": { "$value": { "colorSpace": "srgb", "components": [0, 0, 1], "alpha": 0.8, "hex": "#0000ff" } },
    "modifiedForeground": { "$value": "{color.border.info}" },
    "addedForeground": { "$value": "{color.border.success}" },
    "deletedForeground": { "$value": "{color.border.danger}" },
    "errorForeground": { "$value": "{color.border.danger}" },
    "warningForeground": { "$value": "{color.border.warning}" },
    "infoForeground": { "$value": "{color.border.info}" },
    "bracketMatchForeground": { "$value": "{editorBracketMatch.background}" }
  },
  "editorError": {
    "foreground": { "$value": "{color.font.danger}" },
    "border": {}
  },
  "editorWarning": {
    "foreground": { "$value": "{color.font.warning}" },
    "border": {}
  },
  "editorInfo": {
    "foreground": { "$value": "{color.font.info}" },
    "border": {}
  },
  "editorHint": {
    "foreground": { "$value": "{color.font.active}" },
    "border": {}
  },
  "editorBracketMatch": {
    "background": { "$value": "{color.background.selection.inactive.20}" },
    "border": { "$value": "{color.background.surface.40}" }
  },
  "editorWhitespace": {
    "foreground": { "$value": "{color.font.ghost}" }
  },
  "editorIndentGuide": {
    "background1": { "$value": "{color.border.primary.40}" },
    "activeBackground1": { "$value": "{color.border.primary.50}" }
  },
  "editorRuler": {
    "foreground": { "$value": "{color.border.primary.40}" }
  },
  "editorCodeLens": {
    "foreground": { "$value": "{color.background.surface.20}" }
  },
  "editorUnnecessaryCode": {
    "opacity": { "$value": { "colorSpace": "srgb", "components": [0, 0, 0], "alpha": 0.667, "hex": "#000000" } },
    "border": { "$value": "{color.border.primary.50}" }
  }
}
