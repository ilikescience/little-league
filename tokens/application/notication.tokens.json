{
  "$type": "color",
  "notificationCenter": {
    "border": { "$value": "{color.border.primary.40}" }
  },
  "notificationToast": {
    "border": { "$value": "{color.border.primary.40}" }
  },
  "notificationCenterHeader": {
    "background": { "$value": "{color.background.surface.20}" },
    "foreground": { "$value": "{color.font.primary.50}" }
  },
  "notifications": {
    "background": { "$value": "{color.background.surface.10}" },
    "foreground": { "$value": "{color.font.primary.60}" },
    "border": { "$value": "{color.border.primary.40}" }
  },
  "notificationLink": {
    "foreground": { "$value": "{color.font.link.primary.active}" }
  },
  "notificationsErrorIcon": {
    "foreground": { "$value": "{color.font.danger}" }
  },
  "notificationsWarningIcon": {
    "foreground": { "$value": "{color.font.warning}" }
  },
  "notificationsInfoIcon": {
    "foreground": { "$value": "{color.font.info}" }
  }
}
